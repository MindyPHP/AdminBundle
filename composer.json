{
    "name": "mindy/admin-bundle",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\AdminBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\AdminBundle\\Tests\\": ""
        }
    },
    "require": {
        "php": ">=7.0",
        "mindy/menu": "^1.0"
    },
    "authors": [
        {
            "name": "Maxim Falaleev",
            "email": "max@studio107.ru",
            "homepage": "http://studio107.ru"
        }
    ],
    "require-dev": {
        "mindy/form-bundle": ">=4.0.3",
        "mindy/mindy-bundle": "~4.0",
        "symfony/framework-bundle": "~3.0|~4.0",
        "symfony/security-bundle": "~3.0|~4.0"
    }
}
