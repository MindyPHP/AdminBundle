{
  "name": "frontend_pack",
  "version": "1.0.0",
  "description": "simple frontend pack",
  "scripts": {
    "start": "npm-run-all -p watch webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:js": "eslint --ext .js,.jsx src/**",
    "lint:scss": "sass-lint -c ./scss-lint.yml './scss/**/*.scss' -v -q",
    "webpack": "webpack --progress --colors --watch --config=./webpack.config.js",
    "watch": "gulp watch",
    "build": "cross-env NODE_ENV=production gulp && webpack -p --config=./webpack.config.js"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "bluebird": "^3.4.7",
    "bower": "^1.8.0",
    "browser-sync": "^2.15.0",
    "cross-env": "^5.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "expose-loader": "^0.7.1",
    "fancybox": "^3.0.0",
    "fastclick": "^1.0.6",
    "flexy-framework": "^4.1.7",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-nunjucks": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "magnific-popup": "^1.1.0",
    "noty": "^3.1.3",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "sass-lint": "^1.10.2",
    "webpack": "^2.2.0",
    "webui-popover": "^1.2.17"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "blueimp-file-upload": "^9.19.1",
    "gulp-flatten": "^0.3.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-watch": "^4.3.11",
    "jquery": "^3.2.1",
    "jquery-simplecolorpicker": "^0.3.1",
    "jquery-ui": "^1.12.1",
    "leaflet": "^1.2.0",
    "material-design-icons": "^3.0.1",
    "postcss-csso": "^2.0.0",
    "postcss-flexbugs-fixes": "^3.0.0"
  }
}
